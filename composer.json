{
    "name": "fredtro/mailer-bundle",
    "description": "Symfony2 bundle for sending emails using twig templates.",
    "type": "symfony-bundle",
    "keywords": [
        "mailer",
        "swiftmailer",
        "twigswiftmailer",
        "twigmailer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Frederik Trostorf",
            "email": "fredtrostorf@gmail.ch"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "symfony/twig-bundle": "^2.7 || ^3.0",
        "twig/twig": "^1.28 || ^2.0",
        "symfony/templating": "^2.7 || ^3.0"
    },
    "require-dev": {
        "swiftmailer/swiftmailer": "^4.3 || ^5.0 || ^6.0",
        "symfony/phpunit-bridge": "^2.7 || ^3.0",
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Fredtro\\MailerBundle": ""
        }
    },
    "target-dir": "Fredtro/MailerBundle"
}